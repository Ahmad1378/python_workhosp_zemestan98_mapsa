def func(n , a , b , c) :

    if n == 1 :
        print ("Move from {} to {}".format(a , c))
        return 1

    if n == 2 :
        print ("Move from {} to {}".format( a , b))
        print ("Move from {} to {}".format( a , c ))
        print ("Move from {} to {}".format( b , c))
        return 1

    return func(n-1 , a , c , b) + func(1 , a , b , c ) + func(n-1 , b , a , c)
func( 4 , "1" , "2" , "3")
